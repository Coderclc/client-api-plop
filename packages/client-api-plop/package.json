{
  "name": "client-api-plop",
  "version": "1.7.0",
  "description": "Use ESLint to format generated code with plop.js",
  "keywords": [
    "eslint",
    "plop-action",
    "plop-helper",
    "plop-pack",
    "plop.js",
    "plop",
    "prettier"
  ],
  "license": "MIT",
  "author": "zone",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "LICENSE"
  ],
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "rimraf lib && tsc --noEmit false --outDir lib"
  },
  "peerDependencies": {
    "plop": "^4.0.1"
  },
  "engines": {
    "node": ">=18"
  }
